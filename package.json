  "private": true,
  "engines": {
    "node": ">=8.0.0"
  },
  "scripts": {
    "build": "rimraf dist && react-static build && sw-precache --verbose --config sw-precache.config.js",
    "deploy:production": "NODE_ENV=production npm run build && gh-pages -d dist -b master",
    "deploy:staging": "NODE_ENV=staging npm run build && surge -p dist -d staging.cjpatoilo.com",
    "precommit": "lint-staged",
    "start": "react-static start"
  },
  "lint-staged": {
    "linters": {
      "src/**/*": [
        "prettier-standard",
        "git add"
      ]
    }
  },
  "dependencies": {
    "font-awesome": "4.7.0",
    "google-fonts": "1.0.0",
    "normalize.css": "8.0.1",
    "raven-js": "3.27.2",
    "react": "16.12.0",
    "react-dom": "16.12.0",
    "react-ga": "2.7.0",
    "react-router": "5.1.2",
    "react-static": "7.2.3"
  },
  "devDependencies": {
    "@types/node": "10.17.13",
    "@types/react": "16.9.17",
    "@types/react-dom": "16.9.4",
    "@types/react-hot-loader": "4.1.0",
    "@types/react-router": "5.1.3",
    "@types/webpack-env": "1.14.1",
    "babel-plugin-inline-dotenv": "1.5.0",
    "convert-tsconfig-paths-to-webpack-aliases": "0.9.2",
    "eslint-config-react-tools": "2.0.5",
